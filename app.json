{
  "expo": {
    "name": "Zoodex",
    "slug": "coffuel-zoodex",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "android": {
      "package": "com.coffuel.zoodex",
      "versionCode": 3,
      "permissions": ["CAMERA"]
    },
    "splash": {
      "image": "./assets/mysplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#008000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
